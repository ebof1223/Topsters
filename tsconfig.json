{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "downlevelIteration": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "newTopster/newTopster-styles/CharacterCounterStyles.tsx",
    "newTopster/BackButton.tsx",
    "topster/Album.tsx",
    "topster/NavbarStyles.tsx",
    "topster/Navbar.tsx",
    "topster/topster-styles/ToppingsStyles.tsx",
    "topster/Toppings.tsx",
    "Carousel.tsx"
  ]
}
